{
  "name": "R\u1ea1ng \u0110\u00f4ng Product List",
  "baseSelector": "//div[contains(@class, 'product-card')]",
  "fields": [
    {
      "name": "title",
      "selector": ".//a[@class='product-title']",
      "type": "text"
    },
    {
      "name": "price",
      "selector": ".//span[@class='product-price']",
      "type": "text"
    },
    {
      "name": "image_url",
      "selector": ".//img[contains(@class, 'product-image')]",
      "type": "attribute",
      "attribute": "src"
    },
    {
      "name": "description",
      "selector": "",
      "type": "text"
    }
  ]
}